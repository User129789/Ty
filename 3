task.spawn(function()
    while true do
        task.wait(math.random(8, 20))
        local flash = Instance.new("PointLight")
        flash.Color = Color3.new(1, 1, 1)
        flash.Range = 60
        flash.Brightness = 5
        local thunderPos = Vector3.new(math.random(-100, 100), 300, math.random(-100, 100))
        local thunderPart = Instance.new("Part")
        thunderPart.Size = Vector3.new(1, 1, 1)
        thunderPart.Anchored = true
        thunderPart.CanCollide = false
        thunderPart.Transparency = 1
        thunderPart.Position = thunderPos
        thunderPart.Parent = workspace
        flash.Parent = thunderPart
        task.delay(0.15, function() flash:Destroy() end)
        local thunderSound = Instance.new("Sound")
        thunderSound.SoundId = "rbxassetid://911882310"
        thunderSound.Volume = 1
        thunderSound.RollOffMode = Enum.RollOffMode.Linear
        thunderSound.EmitterSize = 100
        thunderSound.Parent = thunderPart
        thunderSound:Play()
        game.Debris:AddItem(thunderPart, 3)
    end
end)

