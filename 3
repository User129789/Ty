local function rainbowify(label)
    task.spawn(function()
        while true do
            local t = os.clock() * 2
            local r = math.sin(t) * 127 + 128
            local g = math.sin(t + 2) * 127 + 128
            local b = math.sin(t + 4) * 127 + 128
            label.TextColor3 = Color3.fromRGB(r, g, b)
            task.wait(0.05)
        end
    end)
end

rainbowify(TimerLabel)
rainbowify(KillLabel)
rainbowify(MoonLabel)

task.spawn(function()
    local leaderstats = LocalPlayer:WaitForChild("leaderstats", 10)
    if leaderstats then
        local killStat = leaderstats:FindFirstChild("Kills") or leaderstats:FindFirstChild("kills")
        if killStat then
            KillLabel.Text = "Kills: " .. tostring(killStat.Value)
            killStat:GetPropertyChangedSignal("Value"):Connect(function()
                KillLabel.Text = "Kills: " .. tostring(killStat.Value)
            end)
        end
    end
end)

local timeLeft = 90
task.spawn(function()
    while true do
        TimerLabel.Text = "Server Hop en " .. timeLeft .. "s"
        timeLeft -= 1
        task.wait(1)
    end
end)

local function ServerHop()
    local servers = {}
    local requestFunction = syn and syn.request or http_request or request
    if not requestFunction then return end
    local success, response = pcall(function()
        return requestFunction({
            Url = "https://games.roblox.com/v1/games/" .. PlaceId .. "/servers/Public?sortOrder=Desc&limit=100"
        })
    end)
    if not success or not response or not response.Body then return end
    local data = HttpService:JSONDecode(response.Body)
    for _, server in ipairs(data.data) do
        if server.id ~= JobId and server.playing < server.maxPlayers then
            table.insert(servers, server.id)
        end
    end
    if #servers > 0 then
        TeleportService:TeleportToPlaceInstance(PlaceId, servers[math.random(1, #servers)])
    end
end

local function MonitorDeath()
    local function onCharacter(character)
        local humanoid = character:WaitForChild("Humanoid", 5)
        if humanoid then
            humanoid.Died:Connect(ServerHop)
        end
    end
    if LocalPlayer.Character then onCharacter(LocalPlayer.Character) end
    LocalPlayer.CharacterAdded:Connect(onCharacter)
end

MonitorDeath()

task.spawn(function()
    while true do
        task.wait(90)
        timeLeft = 90
        pcall(ServerHop)
    end
end)

getgenv().AutoPunchEnabled = true
local fastHitActive = true

local function equipPunchHand()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    for _, tool in ipairs(LocalPlayer.Backpack:GetChildren()) do
        if tool.Name:lower():find("punch") then
            tool.Parent = char
        end
    end
end

local function getHand()
    local char = LocalPlayer.Character
    if not char then return nil end
    return char:FindFirstChild("RightHand") or char:FindFirstChild("Right Arm")
end

local function autoPunch()
    while fastHitActive do
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("Humanoid") then
            local punch = LocalPlayer.Backpack:FindFirstChild("Punch")
            if punch then
                punch.Parent = char
                if punch:FindFirstChild("attackTime") then
                    punch.attackTime.Value = 0
                end
            end
        end
        task.wait(0.05)
    end
end

task.spawn(autoPunch)

getgenv().AutoKillGlobal = true

if getgenv().AutoKillGlobal then
    print("‚ò†Ô∏è AutoKill Global ACTIVADO")

    local player = game.Players.LocalPlayer
    local function getHand()
        local char = player.Character or player.CharacterAdded:Wait()
        return char:FindFirstChild("RightHand") or char:FindFirstChild("Right Arm")
    end

    local hand = getHand()
    player.CharacterAdded:Connect(function()
        task.wait(1)
        hand = getHand()
    end)

    task.spawn(function()
        while getgenv().AutoKillGlobal do
            if not hand or not hand:IsDescendantOf(workspace) then
                hand = getHand()
            end
            if hand and hand:IsDescendantOf(workspace) then
                for _, other in ipairs(game.Players:GetPlayers()) do
                    if other ~= player and other.Character then
                        local target = other.Character:FindFirstChild("HumanoidRootPart")
                        if target then
                            pcall(function()
                                firetouchinterest(hand, target, 0)
                                task.wait(0.05)
                                firetouchinterest(hand, target, 1)
                            end)
                        end
                    end
                end
            end
            task.wait(0.1)
        end
    end)
else
    print("üõë AutoKill Global DESACTIVADO")
end

task.spawn(function()
    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    pcall(function()
        local tool = char:FindFirstChildOfClass("Tool")
        if tool then
            tool:Activate()
        else
            equipPunchHand()
        end
    end)
end)
